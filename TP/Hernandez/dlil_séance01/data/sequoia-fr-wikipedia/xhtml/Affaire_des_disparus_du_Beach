						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading" lang="fr"><span dir="auto">Affaire des disparus du Beach</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
