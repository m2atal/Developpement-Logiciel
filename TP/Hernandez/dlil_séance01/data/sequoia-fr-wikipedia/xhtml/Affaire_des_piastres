						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading" lang="fr"><span dir="auto">Affaire des piastres</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">Un article de Wikipédia, l&#039;encyclopédie libre.</div>
				<!-- /tagline -->
								<!-- subtitle -->
				<div id="contentSub"></div>
				<!-- /subtitle -->
