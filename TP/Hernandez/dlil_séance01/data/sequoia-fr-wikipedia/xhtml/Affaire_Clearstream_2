						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading" lang="fr"><span dir="auto">Affaire Clearstream 2</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">Un article de Wikipédia, l&#039;encyclopédie libre.</div>
				<!-- /tagline -->
								<!-- subtitle -->
				<div id="contentSub"></div>
				<!-- /subtitle -->
																<!-- jumpto -->
				<div id="jump-to-nav" class="mw-jump">
					Aller à :					<a href="#mw-head">Navigation</a>, 					<a href="#p-search">rechercher</a>
